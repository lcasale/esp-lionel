# Copy this file to secrets.yaml and fill in your values

# WiFi credentials
wifi_ssid: "your_wifi_ssid"
wifi_password: "your_wifi_password"

# Fallback AP password (used when WiFi connection fails)
ap_password: "fallback_password"

# ESPHome API encryption key (generate with: esphome wizard)
api_encryption_key: "your_api_encryption_key_here"

# Web server authentication
web_server_username: "admin"
web_server_password: "your_web_password"

# OTA update password
ota_password: "your_ota_password"

